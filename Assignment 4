#Q3a
mtcars
iter=dim(mtcars)[2]
iter
mtcars_mean=rep(0,iter)
for(i in 1:iter){
  mtcars_mean[i]=mean(mtcars[,i])
}
mtcars_mean

#Q3b
iris
iter=dim(iris)[2]
uniq=c()
for(i in 1:iter){
  uniq_curr=unique(iris[,i])
  uniq=c(uniq, list(uniq_curr) )
}
uniq

