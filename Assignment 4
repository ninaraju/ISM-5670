#Q1c
#for-loop
fib <- numeric(10)
fib[1] <- 0
fib[2] <- 1

for (i in 3:10) {
  fib[i] <- fib[i - 2] - fib[i - 1]
}

print(fib)

#while-loop
fib <- numeric(10)
fib[1] <- 0
fib[2] <- 1

i <- 3

while (i <= 10) {
  fib[i] <- fib[i - 2] - fib[i - 1]
  i <- i + 1
}

print(fib)

#Q1d
#for-loop
fib <- numeric(10)
fib[1] <- 0
fib[2] <- 1
fib[3] <- 1
fib[4] <- 2

for (i in 5:10) {
  fib[i] <- fib[i - 1] + fib[i - 2] + fib[i - 3]
}

print(fib)

#while-loop
fib <- numeric(10)
fib[1] <- 0
fib[2] <- 1
fib[3] <- 1
fib[4] <- 2

i <- 5

while (i <= 10) {
  fib[i] <- fib[i - 1] + fib[i - 2] + fib[i - 3]
  i <- i + 1
}

print(fib)

#Q2
#for-loop
fib <- numeric(30)
fib[1] <- 1
fib[2] <- 1

for (i in 3:30) {
  fib[i] <- fib[i - 1] + fib[i - 2]
}

ratios <- numeric(29)

for (i in 2:30) {
  ratios[i - 1] <- fib[i] / fib[i - 1]
}

print(ratios)
