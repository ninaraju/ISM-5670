#Q1a
#for-loop
fib <- numeric(10)
fib[1] <- 2
fib[2] <- 2

for (i in 3:10) {
  fib[i] <- fib[i - 1] + fib[i - 2]
}

print(fib)

#while-loop
fib <- numeric(10)
fib[1] <- 2
fib[2] <- 2

i <- 3

while (i <= 10) {
  fib[i] <- fib[i - 1] + fib[i - 2]
  i <- i + 1
}

print(fib)

